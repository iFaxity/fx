{
  "private": true,
  "version": "1.1.4",
  "name": "kirei",
  "workspaces": [
    "packages/*"
  ],
  "nyc": {
    "reporter": "lcov",
    "report-dir": "./cypress/coverage",
    "temp-dir": "./.nyc_output",
    "exclude": [
      "cypress"
    ]
  },
  "scripts": {
    "build": "node ./bin/build",
    "ci": "node ./bin/build",
    "clean": "rm -rf packages/*/dist",
    "docs": "typedoc packages/*/src",
    "dev": "vite serve cypress/app -c ./cypress/app/vite.config.js",
    "pretest": "rm -rf .nyc_output",
    "test": "cypress run -C cypress/cypress.json",
    "test:docker": "docker-compose run --rm cypress"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@cypress/code-coverage": "3.8.2",
    "@cypress/webpack-preprocessor": "5.4.8",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-replace": "2.3.3",
    "@strictsoftware/typedoc-plugin-monorepo": "0.3.1",
    "@types/node": "14.14.2",
    "@types/webpack-env": "1.15.3",
    "babel-loader": "8.1.0",
    "babel-plugin-istanbul": "6.0.0",
    "cypress": "5.4.0",
    "dts-bundle-generator": "5.4.0",
    "esbuild": "0.7.19",
    "mochawesome": "6.1.1",
    "rollup": "2.32.1",
    "rollup-plugin-esbuild": "2.5.2",
    "tslib": "2.0.3",
    "typedoc": "0.19.2",
    "typedoc-plugin-exclude-references": "1.0.0",
    "typedoc-plugin-markdown": "3.0.11",
    "typescript": "4.0.3",
    "vite": "1.0.0-rc.6",
    "vite-plugin-istanbul": "1.1.0",
    "webpack": "5.1.3",
    "yargs": "16.1.0"
  }
}
