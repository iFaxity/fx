{
  "private": true,
  "version": "0.0.0",
  "name": "kirei",
  "workspaces": [
    "packages/*"
  ],
  "nyc": {
    "reporter": "lcov",
    "report-dir": "./cypress/coverage",
    "temp-dir": "./.nyc_output",
    "exclude": [
      "cypress"
    ]
  },
  "scripts": {
    "build": "node ./scripts/build.js",
    "ci": "node ./scripts/build.js --release",
    "clean": "rm -rf packages/*/dist",
    "docs": "typedoc packages/*/src",
    "dev": "vite serve cypress/app --config ./cypress/app/vite.config.js",
    "pretest": "rm -rf .nyc_output",
    "test": "node ./scripts/test.js",
    "deploy": "node ./scripts/deploy.js",
    "deploy:dry": "node ./scripts/deploy.js -d"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-typescript": "7.12.7",
    "@cypress/code-coverage": "3.9.1",
    "@cypress/webpack-preprocessor": "5.5.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.1.1",
    "@rollup/plugin-replace": "3.0.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@strictsoftware/typedoc-plugin-monorepo": "0.3.1",
    "@types/node": "16.11.14",
    "@types/webpack-env": "1.16.0",
    "babel-loader": "8.2.2",
    "babel-plugin-istanbul": "6.0.0",
    "cypress": "9.1.1",
    "dts-bundle-generator": "6.2.0",
    "esbuild": "0.8.31",
    "rollup": "2.36.1",
    "rollup-plugin-esbuild": "4.7.2",
    "semantic-release": "18.0.1",
    "semantic-release-monorepo": "7.0.4",
    "toposort": "2.0.2",
    "tslib": "2.1.0",
    "typedoc": "0.20.14",
    "typedoc-plugin-exclude-references": "1.0.0",
    "typedoc-plugin-markdown": "3.4.0",
    "typescript": "4.1.3",
    "vite": "2.0.0-beta.19",
    "vite-plugin-istanbul": "2.0.0",
    "wait-on": "6.0.0",
    "webpack": "5.65.0",
    "yargs": "17.3.0"
  }
}
