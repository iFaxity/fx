{
  "private": true,
  "version": "1.1.4",
  "name": "kirei",
  "workspaces": [
    "packages/*"
  ],
  "nyc": {
    "reporter": "lcov",
    "report-dir": "./cypress/coverage",
    "temp-dir": "./.nyc_output",
    "exclude": [
      "cypress"
    ]
  },
  "scripts": {
    "build": "node ./bin/build",
    "ci": "node ./bin/build",
    "clean": "rm -rf packages/*/dist",
    "docs": "typedoc packages/*/src",
    "dev": "vite serve cypress/app --config ./cypress/app/vite.config.js",
    "pretest": "rm -rf .nyc_output",
    "test": "cypress run -C cypress/cypress.json",
    "test:docker": "docker-compose run --rm cypress"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-typescript": "7.12.7",
    "@cypress/code-coverage": "3.8.5",
    "@cypress/webpack-preprocessor": "5.5.0",
    "@rollup/plugin-commonjs": "17.0.0",
    "@rollup/plugin-node-resolve": "11.0.0",
    "@rollup/plugin-replace": "2.3.4",
    "@strictsoftware/typedoc-plugin-monorepo": "0.3.1",
    "@types/node": "14.14.10",
    "@types/webpack-env": "1.16.0",
    "babel-loader": "8.2.2",
    "babel-plugin-istanbul": "6.0.0",
    "cypress": "6.0.1",
    "dts-bundle-generator": "5.5.0",
    "esbuild": "0.8.17",
    "mochawesome": "6.2.1",
    "rollup": "2.34.0",
    "rollup-plugin-esbuild": "2.6.0",
    "tslib": "2.0.3",
    "typedoc": "0.19.2",
    "typedoc-plugin-exclude-references": "1.0.0",
    "typedoc-plugin-markdown": "3.0.11",
    "typescript": "4.1.2",
    "vite": "1.0.0-rc.13",
    "vite-plugin-istanbul": "1.1.0",
    "webpack": "5.9.0",
    "yargs": "16.1.1"
  }
}
