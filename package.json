{
  "private": true,
  "version": "1.0.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "predev": "npm run build",
    "dev": "webpack-dev-server",
    "build": "tsc -b --force",
    "test": "npm run test:unit && npm run test:e2e && npm run test:report",
    "test:unit": "nyc --silent --no-clean --include=packages/**/* mocha -- test/unit/**/*.js",
    "test:e2e": " cd test/e2e && cypress run",
    "test:report": "nyc report -r=lcovonly -r=text && rm -rf .nyc_output"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.7.4",
    "@types/node": "13.13.2",
    "@types/webpack-env": "1.15.2",
    "cypress": "4.5.0",
    "file-loader": "6.0.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "istanbul-lib-coverage": "3.0.0",
    "js-framework-benchmark-utils": "0.2.5",
    "mocha": "7.1.2",
    "nyc": "15.0.1",
    "ts-loader": "7.0.4",
    "tslib": "2.0.0",
    "typescript": "3.9.2",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  }
}
