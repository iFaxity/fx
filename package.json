{
  "private": true,
  "version": "1.0.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "predev": "npm run build",
    "dev": "webpack-dev-server",
    "prebuild": "rm -rf packages/*/dist && rm -f packages/*/*.tsbuildinfo",
    "build": "tsc --b",
    "test": "npm run test:unit && npm run test:e2e && npm run test:report",
    "test:unit": "nyc --silent --no-clean --include=packages/**/* mocha -- test/unit/**/*.js",
    "test:e2e": " cd test/e2e && cypress run",
    "test:report": "nyc report --reporter=lcovonly && rm -rf .nyc_output"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.7.2",
    "@types/node": "13.13.6",
    "@types/webpack-env": "1.15.2",
    "cypress": "4.5.0",
    "file-loader": "5.1.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "istanbul-lib-coverage": "3.0.0",
    "js-framework-benchmark-utils": "0.2.5",
    "mocha": "7.1.2",
    "nyc": "15.0.1",
    "ts-loader": "6.2.2",
    "tslib": "1.11.1",
    "typescript": "3.8.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  }
}
