{
  "private": true,
  "version": "0.0.0",
  "name": "kirei",
  "workspaces": [
    "packages/*"
  ],
  "nyc": {
    "reporter": "lcov",
    "report-dir": "./cypress/coverage",
    "temp-dir": "./.nyc_output",
    "exclude": [
      "cypress"
    ]
  },
  "scripts": {
    "build": "node ./scripts/build.js",
    "ci": "node ./scripts/build.js --release",
    "clean": "rm -rf packages/*/dist",
    "docs": "typedoc packages/*/src",
    "dev": "vite serve cypress/app --config ./cypress/app/vite.config.js",
    "pretest": "rm -rf .nyc_output",
    "test": "node ./scripts/test.js",
    "deploy": "node ./scripts/deploy.js",
    "deploy:dry": "node ./scripts/deploy.js -d"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@cypress/code-coverage": "3.9.12",
    "@cypress/webpack-preprocessor": "5.11.1",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-node-resolve": "11.2.1",
    "@rollup/plugin-replace": "2.4.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.1",
    "@strictsoftware/typedoc-plugin-monorepo": "0.4.2",
    "@types/node": "14.18.14",
    "@types/webpack-env": "1.16.4",
    "babel-loader": "8.2.5",
    "babel-plugin-istanbul": "6.1.1",
    "cypress": "6.9.1",
    "dts-bundle-generator": "5.9.0",
    "esbuild": "0.14.38",
    "rollup": "2.70.2",
    "rollup-plugin-esbuild": "2.6.1",
    "semantic-release": "17.4.7",
    "semantic-release-monorepo": "7.0.5",
    "toposort": "2.0.2",
    "tslib": "2.4.0",
    "typedoc": "0.22.15",
    "typedoc-plugin-exclude-references": "1.0.0",
    "typedoc-plugin-markdown": "3.12.1",
    "typescript": "4.6.3",
    "vite": "2.9.5",
    "vite-plugin-istanbul": "2.7.1",
    "wait-on": "5.3.0",
    "webpack": "4.46.0",
    "yargs": "16.2.0"
  }
}
