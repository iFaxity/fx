{
  "private": true,
  "version": "1.1.3",
  "name": "kirei",
  "workspaces": [
    "packages/*"
  ],
  "nyc": {
    "reporter": "lcov",
    "report-dir": "./cypress/coverage",
    "temp-dir": "./.nyc_output",
    "exclude": [
      "cypress"
    ]
  },
  "scripts": {
    "build": "tsc -b --force",
    "postbuild": "rollup -c rollup.config.js",
    "build:ci": "tsc -b --force",
    "clean": "rm -rf packages/*/dist",
    "docs": "typedoc packages/*/src",
    "dev": "vite serve cypress/app -c ./cypress/app/vite.config.js",
    "pretest": "rm -rf .nyc_output",
    "test": "cypress run -C cypress/cypress.json",
    "test:docker": "docker-compose run --rm cypress"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/preset-env": "7.11.5",
    "@cypress/code-coverage": "3.8.1",
    "@cypress/webpack-preprocessor": "5.4.5",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-replace": "2.3.3",
    "@strictsoftware/typedoc-plugin-monorepo": "0.3.1",
    "@types/node": "14.11.2",
    "babel-loader": "8.1.0",
    "babel-plugin-istanbul": "6.0.0",
    "cypress": "5.2.0",
    "mochawesome": "6.1.1",
    "rollup": "2.28.2",
    "rollup-plugin-terser": "7.0.2",
    "typedoc": "0.19.1",
    "typedoc-plugin-exclude-references": "1.0.0",
    "typedoc-plugin-markdown": "2.4.2",
    "typescript": "4.0.2",
    "vite": "1.0.0-rc.4",
    "vite-plugin-istanbul": "1.1.0",
    "webpack": "4.44.1"
  }
}
